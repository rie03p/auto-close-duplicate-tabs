{
  "name": "auto-close-duplicate-tabs",
  "displayName": "Auto close duplicate tabs",
  "version": "0.0.2",
  "description": "Automatically closes duplicate tabs based on URL or domain for specific sites.",
  "author": "Rie",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.90.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.1.24",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "webNavigation"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
